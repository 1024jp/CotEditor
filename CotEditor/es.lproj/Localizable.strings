// keyboard key name
"Space" = "Espacio";

// Contextual menu items
"Rename “%@”" = "Renombrar “%@”";
"Duplicate “%@”" = "Duplicar “%@”";
"Restore “%@”" = "Restaurar “%@”";
"Export “%@”…" = "Exportar “%@”…";
"Reveal “%@” in Finder" = "Mostrar “%@” en el Finder";

// filename field label variation for save panel
"Export As:" = "Exportar como:";

// button label for open panel
"Import" = "Importar";

// print header/footer
"Printed on %@" = "Impreso el %@";  // %@ is date
"Last modified on %@" = "Última modificación el %@";  // %@ is date
