// Basic button titles
"OK" = "OK";
"Cancel" = "Annulla";

// words with symbols
" (%@)" = " (%@)";

// encoding name for UTF-8 with BOM (%@ is the system localized name for UTF-8)
"%@ with BOM" = "%@ con BOM";

// syntax name
"None" = "Nessuno";

// migration name for legacy snippet setting
"Insert Text %lld" = "Inserisci testo %lld";

// keyboard key name
"Space" = "Spazio";

// print header/footer
"Printed on %@" = "Stampato su %@";  // %@ is date
"Last modified on %@" = "Ultima modifica il %@";  // %@ is date
