// Basic button titles
"OK" = "好";
"Cancel" = "取消";


// words with symbols
" (%@)" = "（%@）";

// encoding name for UTF-8 with BOM (%@ is the system localized name for UTF-8)
"%@ with BOM" = "%@ with BOM";  // No need to be translated because "UTF-8 with BOM" is acceptable in zh-Hans.

// syntax name
"None" = "无";

// migration name for legacy snippet setting
"Insert Text %lld" = "插入文本%lld";

// keyboard key name
"Space" = "空格";

// print header/footer
"Printed on %@" = "打印：%@";  // %@ is date
"Last modified on %@" = "最终修改：%@";  // %@ is date
