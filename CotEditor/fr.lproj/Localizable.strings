// keyboard key name
"Space" = "Espace";

// Contextual menu items
"Rename “%@”" = "Renommer « %@ »";
"Duplicate “%@”" = "Dupliquer « %@ »";
"Restore “%@”" = "Restaurer « %@ »";
"Export “%@”…" = "Export de « %@ »…";
"Reveal “%@” in Finder" = "Afficher « %@ » dans le Finder";

// filename field label variation for save panel
"Export As:" = "Exporter sous :";

// button label for open panel
"Import" = "Importer";

// print header/footer
"Printed on %@" = "Imprimé le %@";  // %@ is date
"Last modified on %@" = "Modifié la dernière fois le %@";  // %@ is date
