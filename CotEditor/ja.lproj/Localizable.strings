"Export “%@”…" = "“%@”を書き出す…";
"Reveal “%@” in Finder" = "“%@”をFinderに表示";

// filename field label variation for save panel
"Export As:" = "名前:";

// button label for open panel
"Import" = "読み込む";

// print header/footer
"Printed on %@" = "プリント: %@";  // %@ is date
"Last modified on %@" = "最終変更: %@";  // %@ is date
